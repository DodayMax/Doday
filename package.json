{
  "name": "Doday",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build:client": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:coverage": "node scripts/test.js --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start:server": "npm run serve:server",
    "build:server": "npm run build-ts && npm run tslint",
    "serve:server": "node dist/server/src/server.js",
    "watch-node": "nodemon dist/server/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test:server": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc -p server/tsconfig.json",
    "watch-ts": "tsc -p server/tsconfig.json -w",
    "tslint": "tslint -c server/tslint.json -p server/tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/src/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },

{
  "name": "@doday/server",
  "version": "1.0.0",
  "description": "Backend for Doday app",
  "author": "Ivan Smirnov <v-ivsmir@microsoft.com>",
  "homepage": "",
  "license": "ISC",
  "main": "dist/server/src/server.js",
  "files": [
    "src"
  ],
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server/src/server.js",
    "watch-node": "nodemon dist/server/src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc -p tsconfig.json",
    "watch-ts": "tsc -p tsconfig.json -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server/src/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "async": "^2.6.2",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cuid": "^2.1.6",
    "domino": "^2.1.1",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.4",
    "graphql": "^14.0.2",
    "graphql-auth-directives": "^1.0.1",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.5",
    "lusca": "^1.6.1",
    "neo4j-driver": "^1.7.3",
    "neo4j-graphql-js": "^2.1.1",
    "node-fetch": "^2.3.0",
    "nodemailer": "^4.4.1",
    "page-metadata-parser": "^1.1.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "stripe": "^6.28.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.4.1",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/cuid": "^1.3.0",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.91",
    "@types/lusca": "^1.5.0",
    "@types/node": "^10.12.18",
    "@types/nodemailer": "^4.3.4",
    "@types/passport": "^0.4.3",
    "@types/passport-facebook": "^2.1.7",
    "@types/passport-google-oauth": "^1.0.39",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.47.0",
    "@types/stripe": "^6.25.8",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "chalk": "2.4.1",
    "concurrently": "^3.5.1",
    "dotenv-expand": "4.2.0",
    "nodemon": "^1.13.0",
    "supertest": "^4.0.2",
    "tslint": "^5.9.1",
    "typescript": "^3.2.2"
  }
}

{
  "name": "@doday/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js"
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "apollo-boost": "^0.3.1",
    "apollo-client": "^2.5.1",
    "apollo-link-context": "^1.0.17",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cuid": "^2.1.6",
    "fs-extra": "7.0.1",
    "graphql": "^14.2.0",
    "http-proxy-middleware": "^0.19.1",
    "i18next": "^15.0.8",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "identity-obj-proxy": "3.0.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.2",
    "rc-slider": "^8.6.9",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-apollo": "^2.5.3",
    "react-app-polyfill": "^0.2.2",
    "react-datepicker": "^2.3.0",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-google-charts": "^3.0.11",
    "react-i18next": "^10.6.0",
    "react-loadable": "^5.5.0",
    "react-media": "^1.9.2",
    "react-minimal-pie-chart": "^3.5.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^4.0.0",
    "react-virtualized": "^9.21.1",
    "redux-saga": "^1.0.2",
    "resolve": "1.10.0",
    "sfcookies": "^1.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/src/setupTests"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@root(.*)$": "<rootDir>/src$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@styles(.*)$": "<rootDir>/src/styles$1",
      "^@shared(.*)$": "<rootDir>/src/components/shared$1",
      "^@stores(.*)$": "<rootDir>/src/stores$1",
      "^@lib(.*)$": "<rootDir>/src/lib$1",
      "^@lib/utils(.*)$": "<rootDir>/src/lib/utils$1",
      "^@ducks(.*)$": "<rootDir>/src/ducks$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@svgr/webpack": "4.1.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.12.2",
    "@types/react-dom": "16.8.3",
    "@types/cuid": "^1.3.0",
    "@types/i18next": "^12.1.0",
    "@types/lodash": "^4.14.123",
    "@types/moment": "^2.13.0",
    "@types/prop-types": "^15.7.1",
    "@types/query-string": "^6.3.0",
    "@types/rc-slider": "^8.6.3",
    "@types/rc-tooltip": "^3.7.1",
    "@types/react-addons-css-transition-group": "^15.0.5",
    "@types/react-datepicker": "^2.2.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-loadable": "^5.5.1",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.9.0",
    "@types/react-virtualized": "^9.21.2",
    "@types/redux-saga": "^0.10.5",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  }
}
